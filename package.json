{
  "name": "touchlabs-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/DigitalInnovation/touchlabs-webpack.git"
  },
  "devDependencies": {
    "istanbul": "^0.4.5"
  },
  "scripts": {
    "start": "npm-run-all bundle open:src",
    "dev": "npm-run-all bundle dev-parallel",
    "dev-parallel": "npm-run-all --parallel lint:watch open:src",
    "open:src": "node src/server/server.js",
    "bundle": "webpack --config webpack.config.js",
    "lint": "node_modules/.bin/eslint  --color webpack.config.js src",
    "lint:watch": "npm run lint -- --watch --color",
    "test-unit": "istanbul cover -x '**/*.spec.js' _mocha -- -R spec unitTest.config.js"
  },
  "engines": {
    "node": "6.5.0",
    "npm": "3.10.3"
  },
  "author": "Team Mustang",
  "license": "ISC",
  "dependencies": {
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.4.0",
    "eslint-watch": "^2.1.14",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "handlebars-loader": "^1.4.0",
    "html-loader": "^0.4.4",
    "json-loader": "^0.5.4",
    "node-sass": "^3.9.3",
    "npm-run-all": "^3.1.0",
    "path": "^0.12.7",
    "sass-loader": "^4.0.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "chai": "^3.5.0",
    "mocha": "^3.0.2",
    "require-all": "^2.0.0"
  }
}
